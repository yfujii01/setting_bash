[user]
	name = yfujii01
	email = y-fujii@rhizome-dev.sakura.ne.jp
[diff]
    tool = default-difftool
[difftool "default-difftool"]
    path = D:/fujii/tool/winmerge-2.14.0-jp-148-x64-exe/WinMerge/WinMergeU.exe
    cmd = \"D:/fujii/tool/winmerge-2.14.0-jp-148-x64-exe/WinMerge/WinMergeU.exe\" -f \"*.*\" -e -u -r \"$LOCAL\" \"$REMOTE\"
[merge]
    tool = default-mergetool
[mergetool "default-mergetool"]
    path = D:/fujii/tool/winmerge-2.14.0-jp-148-x64-exe/WinMerge/WinMergeU.exe
    cmd = \"D:/fujii/tool/winmerge-2.14.0-jp-148-x64-exe/WinMerge/WinMergeU.exe\" -e -u \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
[alias]
    a = add .
    c = commit
    p = push origin
    d = difftool -y -d -t default-difftool
    m = mergetool -y -t default-mergetool
    s = status --short
    k = !gitk --all --date-order --simplify-by-decoration
    l = log --oneline --graph --decorate --all --date-order --simplify-by-decoration
[core]
	# editor = code --wait -new-window
	editor = 'C:/Program Files (x86)/vim/vim80/vim.exe' -c 'set fenc=utf-8'
	quotepath = false
	autocrlf = false
